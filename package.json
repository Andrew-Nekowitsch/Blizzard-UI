{
	"name": "blizzard-ui",
	"version": "1.0.0",
	"description": "An attempt to immitate blizzard's uis",
	"private": "true",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.config.js --env env=dev",
		"build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --env env=prod --progress --color",
		"deploy": "npm run build && gh-pages -d dist",
		"lint": "eslint src --ext .ts --ext .tsx"
	},
	"repository": {
		"type": "git",
		"url": "git+https://andrew-nekowitsch.github.io/Blizzard-UI/.git"
	},
